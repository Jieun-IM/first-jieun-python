## 3항 조건문 ##
jumsu = 55
res = ''

res = '합격' if jumsu >= 60 else '불합격'
print(res)

print('---------------------------------')
## if문으로 리스트 안에 없는 숫자 찾기

import random

numbers = []
for num in range(0,10):
    numbers.append(random.randrange(0,10))

print("생성된 리스트: ", numbers)

for num in range(0,10):
    if num not in numbers:
        print("숫자 %d는(은) 리스트에 없네요 ㅠㅠ" %num)